{
  "name": "fastq_to_counts",
  "description": "Perform count quantification on fastq files with Salmon",
  "tags": ["genomics", "transcriptomics", "rna-seq", "fastq"],
  "authors": ["tercen"],
  "urls": ["https://github.com/tercen/fastq_to_counts_docker_operator"],
  "container": "fcadete/fastq_to_counts_operator:0.0.3",
  "properties": [
    {
    "kind": "EnumeratedProperty",
    "name": "paired_end",
    "defaultValue": "yes",
    "values": [
      "yes",
      "no"    ],
    "description": "Is the sequencing paired end?"
  },
  {
    "kind": "EnumeratedProperty",
    "name": "species",
    "defaultValue": "H. sapiens",
    "values": [
      "H. sapiens",
      "M. musculus"
    ],
    "description": "Sample species"
  }
  ]
}
